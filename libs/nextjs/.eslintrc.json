{
  "extends": [
    "plugin:@nrwl/nx/react",
    "../../.eslintrc.json",
    "@valtech-ch/eslint-config/plugins/nextjs",
    "@valtech-ch/eslint-config/rules/nextjs"
  ],
  "parserOptions": {
    "project": "./tsconfig.*?.json"
  },
  "ignorePatterns": ["!**/*"],
  "overrides": [
    {
      "files": ["*.ts", "*.tsx", "*.js", "*.jsx"],
      "rules": {}
    },
    {
      "files": ["*.ts", "*.tsx"],
      "rules": {}
    },
    {
      "files": ["*.js", "*.jsx"],
      "rules": {}
    }
  ]
}
