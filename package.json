{
  "name": "ring",
  "version": "0.0.0",
  "license": "MIT",
  "private": true,
  "lint-staged": {
    "*.{json,yml,yaml,css,scss,md,mdx,graphql}": [
      "yarn format:write --uncommitted"
    ],
    "*.{js,jsx,ts,tsx}": [
      "yarn format:write --uncommitted",
      "yarn affected:tsc",
      "yarn affected:lint"
    ]
  },
  "scripts": {
    "nx": "nx",
    "start": "nx serve",
    "build": "nx build",
    "test": "nx test",
    "lint": "nx workspace-lint && nx lint",
    "e2e": "nx e2e",
    "affected:apps": "nx affected:apps --base=origin/main",
    "affected:libs": "nx affected:libs --base=origin/main",
    "affected:build": "nx affected:build --base=origin/main",
    "affected:e2e": "nx affected:e2e --base=origin/main",
    "affected:test": "nx affected:test --base=origin/main",
    "affected:lint": "nx affected:lint --base=origin/main",
    "affected:tsc": "nx affected --target=tsc --base=origin/main",
    "affected:dep-graph": "nx affected:dep-graph --base=origin/main",
    "affected": "nx affected",
    "format": "nx format:write",
    "format:write": "nx format:write",
    "format:check": "nx format:check",
    "link:list": "( ls -l node_modules ; ls -l node_modules/@* ) | grep ^l",
    "update": "nx migrate latest",
    "workspace-generator": "nx workspace-generator",
    "dep-graph": "nx dep-graph",
    "help": "nx help",
    "prepare": "husky install",
    "link:packages": "bash ./tools/link-packages.sh",
    "watch": "run-p build:ring build:ring-storyblok",
    "build:ring": "bash ./tools/build-ring.sh",
    "build:ring-contentful": "bash ./tools/build-ring-contentful.sh",
    "build:ring-storyblok": "bash ./tools/build-ring-storyblok.sh"
